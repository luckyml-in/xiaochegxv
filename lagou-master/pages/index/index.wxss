/**index.wxss**/
/**  顶部-start  **/
.custom-info {
  display: flex;
  justify-content: space-between;
  padding: 0 15px;
  border-bottom: 1px solid #e8e8e8;
  height: 43px;
  line-height: 43px;
  color: #555;
  font-size: 14px;
}

.go {
  display: flex;
  align-items: center;
  margin-top: 6px;
  padding: 0 20px;
  height: 30px;
  line-height: 30px;
  color: #00b38a;
  border-radius: 15px;
  background-color: #f5f5f5;
}

.edit {
  margin-right: 2px;
  width: 10px;
  height: 10px;
}
/**  顶部-end  **/
.list-more {
  height: 50px;
  line-height: 50px;
  text-align: center;
  font-size: 15px;
  background-color: #fafafa;
}
